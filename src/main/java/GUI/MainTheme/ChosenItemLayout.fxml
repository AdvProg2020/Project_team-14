<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox alignment="CENTER_LEFT" spacing="5.0" styleClass="chosen-items" stylesheets="@../StyleSheets/MainThemStyle.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label alignment="CENTER" prefHeight="25.0" text="factor">
         <font>
            <Font size="16.0" />
         </font>
         <padding>
            <Insets left="3.0" />
         </padding>
         <HBox.margin>
            <Insets bottom="3.0" left="5.0" top="3.0" />
         </HBox.margin>
      </Label>
      <Button alignment="CENTER" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="28.0" prefWidth="25.0">
         <graphic>
            <ImageView fitHeight="15.0" fitWidth="14.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@resources/close.png" />
               </image>
            </ImageView>
         </graphic>
         <HBox.margin>
            <Insets />
         </HBox.margin>
         <padding>
            <Insets left="2.0" right="5.0" />
         </padding>
      </Button>
   </children>
</HBox>
